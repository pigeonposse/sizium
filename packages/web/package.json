{
	"name": "@sizium/web",
	"version": "0.2.0",
	"private": true,
	"description": "Get the actual size of any local or remote package",
	"type": "module",
	"scripts": {
		"build": "dovenv favicon --silent && vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"preview": "vite preview",
		"test": "pnpm run test:e2e",
		"test:e2e": "playwright test"
	},
	"dependencies": {
		"@sizium/core": "workspace:*"
	},
	"devDependencies": {
		"@ascii-kit/image": "0.0.6",
		"@iconify-json/fa6-brands": "1.2.5",
		"@iconify-json/fa6-solid": "1.2.3",
		"@macfja/svelte-persistent-store": "2.4.2",
		"@svaio/media": "0.0.11",
		"@svaio/meta": "0.0.11",
		"@svaio/pwa": "0.0.11",
		"@svaio/sitemap": "0.0.11",
		"@svaio/unocss": "0.0.11",
		"@sveltejs/adapter-cloudflare": "7.1.0",
		"@sveltejs/kit": "2.25.1",
		"svelte": "5.36.13",
		"svelte-check": "4.3.0"
	}
}
